    "target": "es2016",
    "module": "ESNext",
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "types": ["vitest/globals"],
  "include": ["src/**/*.ts"]
}
